<h1>Edit Entry</h1>


<%= form_with model: @blog, method: :patch do |form| %>

  <%= form.label :title %>
  <%= form.text_field :title %>

  <br>
  <%= form.label :content %>
  <%= form.text_field :content %>

  <br>
  <%= form.submit 'Update Blog' %>
<% end %>




<%= link_to 'Return to Blog List', blogs_path%>